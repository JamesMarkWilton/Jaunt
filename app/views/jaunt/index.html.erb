<div class="container">
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <% @jaunts.each do |jaunt| %>
      <div class="panel panel-info">
        <div class="panel-heading">
          <h2> <%= link_to jaunt.title, show_jaunt_path(jaunt) %> </h2>
        </div>
        <div class="panel-body">
          <h2> <%= jaunt.description  %></h2>
        </div>
      </div>
    <% end %>
  </div>
</div>
</div>
